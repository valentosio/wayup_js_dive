//Задание 1: 
const town = 'Ternivka'; 
const country = 'Ukraine'; 
let population = 50000; 
let stadion = false; 
console.log(town); 
console.log(country); 
console.log(population); 
console.log(stadion); 

//Задание 2: 
let height = 40; 
let width = 70; 
let square = height * width; 
console.log(square); 

 //Задание 3: 
let time = 2; 
let speedOfFirst = 95; 
let speedOfSecond = 114; 
let distance1 = time * speedOfFirst; 
let distance2 = time * speedOfSecond; 
let fullDistance = distance1 + distance2; 
console.log(fullDistance); 

//Задание 4: 
const randomNumber = Math.floor(Math.random() * 100); 
console.log(randomNumber); 
if(randomNumber < 20) { 
    console.log('"randomNumber меньше 20"') 
} else if(randomNumber > 50) { 
    console.log('"randomNumber больше 50"') 
} else { 
    console.log('"randomNumber больше 20, и меньше 50"') 
} 

//Задание 5: 
const randomNumber = Math.floor(Math.random() * 100); 
console.log(randomNumber);
switch(true) { 
    case (randomNumber < 20) : 
        console.log('"randomNumber меньше 20"') 
        break;
    case (randomNumber > 50) : 
        console.log('"randomNumber больше 50"') 
        break; 
    default : 
        console.log('"randomNumber больше 20, и меньше 50"') 
        break; 
}
